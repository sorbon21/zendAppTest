<div class="row justify-content-center">
    <div class="col-12">
        <nav aria-label="Page navigation " class="pagination">
            <ul class="list-group">

                {% for item in pages.pagesInRange %}
                    {% if pages.current == item %}

                        <li class="page-item active">
                              <span class="page-link">
                                {{ item }}
                                <span class="sr-only">(current)</span>
                              </span>
                        </li>
                    {% else %}
                        <li class="page-item"><a class="page-link" href="/latest-products/{{ item}}">{{ item }}</a></li>
                    {% endif %}


                {% endfor %}

            </ul>
        </nav>

    </div>
</div>